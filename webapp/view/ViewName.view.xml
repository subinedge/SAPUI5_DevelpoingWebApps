<mvc:View controllerName="projectNamespace.projectName.controller.ViewName" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" displayBlock="false" xmlns="sap.m">
	<App>
		<pages>
			<Page title="SAPUI5 app development">
				<content>
					<IconTabBar id="idTopLevelIconTabBar" class="sapUiResponsiveContentPadding" selectedKey="db">
						<items>
							<IconTabFilter id="start" text="Getting Started">
								<core:Fragment fragmentName="projectNamespace.projectName.fragments.iconTabBar1" type="XML"/>
							</IconTabFilter>
							<IconTabFilter id="finish" text="Containers" key="db">
								<core:Fragment fragmentName="projectNamespace.projectName.fragments.iconTabBar2" type="XML"/>
							</IconTabFilter>
							<IconTabFilter id="form" text="Layout Form">
								<core:Fragment fragmentName="projectNamespace.projectName.fragments.iconTabBar3" type="XML"/>
							</IconTabFilter>
							<IconTabFilter text="Data Binding">
								<content>
									<core:Fragment fragmentName="projectNamespace.projectName.fragments.iconTabBar4" type="XML"/>
								</content>
							</IconTabFilter>
						</items>
					</IconTabBar>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>